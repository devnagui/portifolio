package br.com.devnagui.project.entities;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;
import javax.validation.constraints.NotNull;

@Entity
@Table(name = "HISTORICO_HABILITA_FISCAL",schema="PROJECT")
public class HistoricoHabilitaFiscal implements Serializable {

	private static final long serialVersionUID = -89317449534669886L;
	
	@Id
	@Column(name = "SEQ_HIST_HABILITA_FISCAL")
	@GeneratedValue(generator = "SQ_HISTORICO_HABILITA_FISCAL", strategy = GenerationType.SEQUENCE)
	@SequenceGenerator(name = "SQ_HISTORICO_HABILITA_FISCAL", sequenceName = "SQ_HISTORICO_HABILITA_FISCAL", allocationSize = 1, initialValue = 1)
	private Long id;
	
	@ManyToOne(optional = false)
	@JoinColumn(name = "SEQ_FISCAL")
	private Fiscal fiscal;
	
	@Column(name = "DAT_HABILITACAO")
	@Temporal(TemporalType.DATE)
	@NotNull
	private Date dataHabilitacao;
	
	@Column(name = "DAT_DESABILITACAO")
	@Temporal(TemporalType.DATE)
	private Date dataDesabilitacao;

	public HistoricoHabilitaFiscal(){}
	
	public HistoricoHabilitaFiscal(Fiscal fiscal, Date dataHabilitacao) {
		super();
		this.fiscal = fiscal;
		this.dataHabilitacao = dataHabilitacao;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Fiscal getFiscal() {
		return fiscal;
	}

	public void setFiscal(Fiscal fiscal) {
		this.fiscal = fiscal;
	}

	public Date getDataHabilitacao() {
		return dataHabilitacao;
	}

	public void setDataHabilitacao(Date dataHabilitacao) {
		this.dataHabilitacao = dataHabilitacao;
	}

	public Date getDataDesabilitacao() {
		return dataDesabilitacao;
	}

	public void setDataDesabilitacao(Date dataDesabilitacao) {
		this.dataDesabilitacao = dataDesabilitacao;
	}
}