package br.gov.ce.sefaz.project.entities;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.Lob;
import javax.persistence.OneToOne;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;

/**
 * 
 * @author 006158C3
 * 
 */
@Entity
@Table(name = "IMPORTACAO_POSTO_ANP",schema="PROJECT")
@SequenceGenerator(name = "SQ_IMPORTACAO_POSTO_ANP", sequenceName = "SQ_IMPORTACAO_POSTO_ANP", allocationSize = 1, initialValue = 1)
public class ImportacaoDadosAnp implements Serializable {

	private static final long serialVersionUID = 1L;

	@Id
	@Column(name = "SEQ_IMPORTACAO_POSTO_ANP")
	@GeneratedValue(generator = "SQ_IMPORTACAO_POSTO_ANP", strategy = GenerationType.SEQUENCE)
	private Long id;

	@OneToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "SEQ_USUARIO")
	private Usuario usuario;

	@Column(name = "DAT_PROCESSAMENTO")
	@Temporal(TemporalType.TIMESTAMP)
	private Date dataProcessamento;

	@Column(name = "TXT_LOG_PROCESSAMENTO")
	@Lob
	private byte[] processamento;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Usuario getUsuario() {
		return usuario;
	}

	public void setUsuario(Usuario usuario) {
		this.usuario = usuario;
	}

	public Date getDataProcessamento() {
		return dataProcessamento;
	}

	public void setDataProcessamento(Date dataProcessamento) {
		this.dataProcessamento = dataProcessamento;
	}

	public byte[] getProcessamento() {
		return processamento;
	}

	public void setProcessamento(byte[] processamento) {
		this.processamento = processamento;
	}

}
