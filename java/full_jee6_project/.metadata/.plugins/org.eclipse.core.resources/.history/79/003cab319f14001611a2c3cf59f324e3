package br.com.devnagui.project.vo;

import java.io.Serializable;

import br.com.devnagui.project.entities.ProdutoCombustivel;

public class SomatorioVolumeEntradaPorProdutoCombustivelVO implements Serializable {
    
    /**
     * Serial UID.
     */
    private static final long serialVersionUID = 6424705963786683296L;


    public SomatorioVolumeEntradaPorProdutoCombustivelVO(ProdutoCombustivel produtoCombustivel, Long somatorioVolume) {
        super();
        this.produtoCombustivel = produtoCombustivel;
        this.somatorioVolume = somatorioVolume;
    }
    
    private ProdutoCombustivel produtoCombustivel;
    private Long somatorioVolume;
    
    
    public ProdutoCombustivel getProdutoCombustivel() {
        return produtoCombustivel;
    }
    public void setProdutoCombustivel(ProdutoCombustivel produtoCombustivel) {
        this.produtoCombustivel = produtoCombustivel;
    }
    public Long getSomatorioVolume() {
        return somatorioVolume;
    }
    public void setSomatorioVolume(Long somatorioVolume) {
        this.somatorioVolume = somatorioVolume;
    }
    
    
}
