package br.com.devnagui.project.enums;

/**
 * 
 * Criado por @author 006159C0 em 06/05/2013
 *
 */
public enum StatusRotaVersao {

	ATIVA(1), EM_EDICAO(2), DESATIVADA(3);

	private int codigoSituacao = 0;

	/**
	 * 
	 */
	private StatusRotaVersao(final int codSituacao) {
		this.codigoSituacao = codSituacao;
	}

	/**
	 * @return the codigoSituacao
	 */
	public Integer getCodigoSituacao() {
		return codigoSituacao;
	}
}