package br.com.devnagui.project.dto;

import java.io.Serializable;

/**
 * Criado por @author 006157C6 em 18/03/2013
 * 
 */
public class PostoCombustivelDTO implements Serializable {

	// ATRIBUTOS E CONSTRUTORES
	private Long id;
	private Long cnpj;
	private String razaoSocial;

	// METODOS PRIVADOS

	// GETS E SETS
	/**
	 * 
	 */
	private static final long serialVersionUID = 4643791250376578722L;

	/**
	 * 
	 */
	public PostoCombustivelDTO() {
	}

	/**
	 * @param cpnj
	 * @param razaoSocial
	 */
	public PostoCombustivelDTO(Long cpnj, String razaoSocial) {
		super();
		this.cnpj = cpnj;
		this.razaoSocial = razaoSocial;
	}

	/**
	 * @param cpnj
	 * @param razaoSocial
	 */
	public PostoCombustivelDTO(Long id, Long cpnj, String razaoSocial) {
		super();
		this.id = id;
		this.cnpj = cpnj;
		this.razaoSocial = razaoSocial;
	}

	// METODOS PUBLICOS

	/**
	 * @return the razaoSocial
	 */
	public String getRazaoSocial() {
		return razaoSocial;
	}

	/**
	 * @return the cnpj
	 */
	public Long getCnpj() {
		return cnpj;
	}

	/**
	 * @param cnpj
	 *            the cnpj to set
	 */
	public void setCnpj(Long cnpj) {
		this.cnpj = cnpj;
	}

	/**
	 * @param razaoSocial
	 *            the razaoSocial to set
	 */
	public void setRazaoSocial(String razaoSocial) {
		this.razaoSocial = razaoSocial;
	}

	/**
	 * @return the id
	 */
	public Long getId() {
		return id;
	}

	/**
	 * @param id
	 *            the id to set
	 */
	public void setId(Long id) {
		this.id = id;
	}

	/*
	 * (non-Javadoc)
	 * 
	 * @see java.lang.Object#hashCode()
	 */
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((cnpj == null) ? 0 : cnpj.hashCode());
		result = prime * result + ((razaoSocial == null) ? 0 : razaoSocial.hashCode());
		return result;
	}

	/*
	 * (non-Javadoc)
	 * 
	 * @see java.lang.Object#equals(java.lang.Object)
	 */
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (!(obj instanceof PostoCombustivelDTO))
			return false;
		PostoCombustivelDTO other = (PostoCombustivelDTO) obj;
		if (cnpj == null) {
			if (other.cnpj != null)
				return false;
		} else if (!cnpj.equals(other.cnpj))
			return false;
		if (razaoSocial == null) {
			if (other.razaoSocial != null)
				return false;
		} else if (!razaoSocial.equals(other.razaoSocial))
			return false;
		return true;
	}

	/*
	 * (non-Javadoc)
	 * 
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "Cnpj: " + cnpj + " - Razao Social: " + razaoSocial;
	}

}
