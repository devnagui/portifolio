package br.com.devnagui.project.enums;

public enum SituacaoRegistroNFE {
	INFORMADA_MANUALMENTE("Informada Manualmente"), IMPORTADA_NFECORP("Importada da NFECORP");

	private String descricao;

	/**
	 * 
	 */
	private SituacaoRegistroNFE(final String descricao) {
		this.descricao = descricao;
	}
	
	public String getDescricao() {
		return descricao;
	}

}
