package br.gov.ce.sefaz.project.enums;

public enum SituacaoVisita {
	ABERTA("Aberta"), FECHADA("Fechada"), ESTOQUE_EM_CONFORMIDADE(
			"Estoque em Conformidade"), ESTOQUE_NAO_CONFORME(
			"Estoque não Conforme");

	private String descricao;

	/**
	 * 
	 */
	private SituacaoVisita(final String descricao) {
		this.descricao = descricao;
	}
	
	public String getDescricao() {
		return descricao;
	}

}
