package br.gov.ce.sefaz.project.vo;

import java.io.Serializable;

import br.gov.ce.sefaz.project.entities.CombustivelVisita;
import br.gov.ce.sefaz.project.entities.ProdutoCombustivel;
import br.gov.ce.sefaz.project.entities.Visita;

public class CombustivelVisitaVO implements Serializable {

	private static final long serialVersionUID = -3034314148136910956L;
	
	private ProdutoCombustivel produtoCombustivel;

	private CombustivelVisita combustivelVisita;
	
	private Visita visita;

	/**
	 * @return the produtoCombustivel
	 */
	public ProdutoCombustivel getProdutoCombustivel() {
		return produtoCombustivel;
	}

	/**
	 * @param produtoCombustivel 
	 * 				the produtoCombustivel to set
	 */
	public void setProdutoCombustivel(ProdutoCombustivel produtoCombustivel) {
		this.produtoCombustivel = produtoCombustivel;
	}

	/**
	 * @return the combustivelVisita
	 */
	public CombustivelVisita getCombustivelVisita() {
		return combustivelVisita;
	}

	/**
	 * @param combustivelVisita 
	 * 				the combustivelVisita to set
	 */
	public void setCombustivelVisita(CombustivelVisita combustivelVisita) {
		this.combustivelVisita = combustivelVisita;
	}

	/**
	 * @return the visita
	 */
	public Visita getVisita() {
		return visita;
	}

	/**
	 * @param visita 
	 * 				the visita to set
	 */
	public void setVisita(Visita visita) {
		this.visita = visita;
	}
}
