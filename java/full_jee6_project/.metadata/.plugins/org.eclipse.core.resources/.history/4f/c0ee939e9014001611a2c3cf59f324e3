package br.gov.ce.sefaz.project.dto;

import java.io.Serializable;

/**
 * Criado por @author 006157C6 em 17/04/2013
 * 
 */
public class ContribuinteDTO implements Serializable {

	// ATRIBUTOS E CONSTRUTORES

	/**
	 * 
	 */
	private static final long serialVersionUID = -2525678966728442476L;

	/**
	 * @param id
	 * @param situacaoContribuinte
	 */
	public ContribuinteDTO(Long id, Long situacaoContribuinte) {
		super();
		this.id = id;
		this.situacaoContribuinte = situacaoContribuinte;
	}

	/**
	 * 
	 */
	public ContribuinteDTO() {
	}

	private Long id;
	private Long situacaoContribuinte;

	/**
	 * @return the id
	 */
	public Long getId() {
		return id;
	}

	/**
	 * @param id
	 *            the id to set
	 */
	public void setId(Long id) {
		this.id = id;
	}

	/**
	 * @return the situacaoContribuinte
	 */
	public Long getSituacaoContribuinte() {
		return situacaoContribuinte;
	}

	/**
	 * @param situacaoContribuinte
	 *            the situacaoContribuinte to set
	 */
	public void setSituacaoContribuinte(Long situacaoContribuinte) {
		this.situacaoContribuinte = situacaoContribuinte;
	}

	// METODOS PUBLICOS

	// METODOS PRIVADOS

	// GETS E SETS

}
