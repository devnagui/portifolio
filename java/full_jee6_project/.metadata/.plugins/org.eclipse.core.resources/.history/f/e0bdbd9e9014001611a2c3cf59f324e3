package br.gov.ce.sefaz.project.enums;

public enum SituacaoFiscal {
	
	DESABILITADO(0, "Desabilitado"), HABILITADO(1, "Habilitado");
	
	private int valor;
	private String descricao;
	
	private SituacaoFiscal(final int valor, final String descricao){
		this.valor = valor;
		this.descricao = descricao;
	}

	public int getValor() {
		return valor;
	}

	public String getDescricao() {
		return descricao;
	}
}
