package br.gov.ce.sefaz.project.vo;

import java.io.Serializable;

import br.gov.ce.sefaz.project.entities.MedicaoTanque;
import br.gov.ce.sefaz.project.entities.Tanque;

public class MedicaoTanqueVO implements Serializable {

	private static final long serialVersionUID = 3363363921724371181L;
	
	private Tanque tanque;
	private MedicaoTanque medicaoTanqueAtual; 
	private MedicaoTanque medicaoTanqueAnterior;

	
	public MedicaoTanqueVO() {
		medicaoTanqueAtual = new MedicaoTanque();
	}
	
	/**
	 * @return the tanque
	 */
	public Tanque getTanque() {
		return tanque;
	}
	/**
	 * @param tanque 
	 * 				the tanque to set
	 */
	public void setTanque(Tanque tanque) {
		this.tanque = tanque;
	}

	/**
	 * @return the medicaoTanqueAnterior
	 */
	public MedicaoTanque getMedicaoTanqueAnterior() {
		return medicaoTanqueAnterior;
	}
	/**
	 * @param medicaoTanqueAnterior 
	 * 				the medicaoTanqueAnterior to set
	 */
	public void setMedicaoTanqueAnterior(MedicaoTanque medicaoTanqueAnterior) {
		this.medicaoTanqueAnterior = medicaoTanqueAnterior;
	}
	
    /**
	 * @return the medicaoTanqueAtual
	 */
	public MedicaoTanque getMedicaoTanqueAtual() {
		return medicaoTanqueAtual;
	}
	/**
	 * @param medicaoTanqueAtual 
	 * 				the medicaoTanqueAtual to set
	 */
	public void setMedicaoTanqueAtual(MedicaoTanque medicaoTanqueAtual) {
		this.medicaoTanqueAtual = medicaoTanqueAtual;
	}
	
	@Override
    public int hashCode() {
        final int prime = 31;
        int result = 1;
        result = prime * result + ((medicaoTanqueAnterior == null) ? 0 : medicaoTanqueAnterior.hashCode());
        result = prime * result + ((tanque == null) ? 0 : tanque.hashCode());
        return result;
    }
    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        MedicaoTanqueVO other = (MedicaoTanqueVO) obj;
        if (medicaoTanqueAnterior == null) {
            if (other.medicaoTanqueAnterior != null)
                return false;
        } else if (!medicaoTanqueAnterior.equals(other.medicaoTanqueAnterior))
            return false;
        if (tanque == null) {
            if (other.tanque != null)
                return false;
        } else if (!tanque.equals(other.tanque))
            return false;
        return true;
    }
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "MedicaoTanqueVO [tanque=" + tanque + ", medicaoTanqueAtual=" + medicaoTanqueAtual + ", medicaoTanqueAnterior=" + medicaoTanqueAnterior + "]";
	}
	
}
