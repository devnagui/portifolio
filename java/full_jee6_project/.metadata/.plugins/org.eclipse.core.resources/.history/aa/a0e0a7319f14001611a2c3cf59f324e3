package br.com.devnagui.project.vo;

import java.io.Serializable;

import br.com.devnagui.project.entities.ProdutoCombustivel;
import br.com.devnagui.project.enums.SituacaoVisita;

public class CalculoConformidadeProdutoCombustivelVO implements Serializable{

    /**
     * Serial UID.
     */
    private static final long serialVersionUID = -704648585730802455L;
    private ProdutoCombustivel produtoCombustivel;
    
    private Long volumeAtual=0l;
    private Long volumeAnterior=0l;
    private Long volumeEntrada=0l;
    private Long volumeSaida=0l;
    
    private Long volumeIdeal=0l;
    private Long volumeOmisso=0l;
    private Double percentualOmissao=0d;
    
    private SituacaoVisita situacaoEstoque;

    public CalculoConformidadeProdutoCombustivelVO() {
    }
    

    public Long getVolumeOmisso() {
        return volumeOmisso;
    }

    public Double getPercentualOmissao() {
      
        return percentualOmissao;
    }
    
    public void setVolumeOmisso(Long volumeOmisso) {
        this.volumeOmisso = volumeOmisso;
    }
    
    public void setPercentualOmissao(Double percentualOmissao) {
        this.percentualOmissao = percentualOmissao;
    }
    
    //-----------------------------------------GETS AND SETS--------------------------------------------------------
    

    public Long getVolumeEntrada() {
        return volumeEntrada;
    }

    public void setVolumeEntrada(Long volumeEntrada) {
        this.volumeEntrada = volumeEntrada;
    }

    public Long getVolumeIdeal() {
        return volumeIdeal;
    }

    public void setVolumeIdeal(Long volumeIdeal) {
        this.volumeIdeal = volumeIdeal;
    }


    public ProdutoCombustivel getProdutoCombustivel() {
        return produtoCombustivel;
    }


    public void setProdutoCombustivel(ProdutoCombustivel produtoCombustivel) {
        this.produtoCombustivel = produtoCombustivel;
    }


    public Long getVolumeAnterior() {
        return volumeAnterior;
    }


    public void setVolumeAnterior(Long volumeAnterior) {
        this.volumeAnterior = volumeAnterior;
    }

    public Long getVolumeSaida() {
        return volumeSaida;
    }

    public void setVolumeSaida(Long volumeSaida) {
        this.volumeSaida = volumeSaida;
    }

    public Long getVolumeAtual() {
        return volumeAtual;
    }

    public void setVolumeAtual(Long volumeAtual) {
        this.volumeAtual = volumeAtual;
    }


    public SituacaoVisita getSituacaoEstoque() {
        return situacaoEstoque;
    }


    public void setSituacaoEstoque(SituacaoVisita situacaoEstoque) {
        this.situacaoEstoque = situacaoEstoque;
    }
}